def unique_elements(nums):
    return list(set(nums))

# Test cases
print(unique_elements([3, 7, 3, 5, 2, 5, 9, 2]))  # Output: [2, 3, 5, 7, 9]
print(unique_elements([-1, 2, -1, 3, 2, -2]))  # Output: [2, 3, -2, -1]
print(unique_elements([1000000, 999999, 1000000]))  # Output: [1000000, 999999]
